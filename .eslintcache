es[b] === squares[c]) {\r\n                return squares[a]\r\n            }\r\n        }\r\n        return null\r\n    }\r\n\r\n    let victory;\r\n    const win = winner(squares);\r\n    if(win) {\r\n        victory = 'The winner is ' + (playerXTurn? 'O' : 'X')\r\n    }\r\n\r\n    return (\r\n        <div className = 'body'>\r\n                <div className = 'game'>\r\n                <div>\r\n                    <Board\r\n                    onClick = {(i)=>handleClick(i)}\r\n                    squares = {squares}\r\n                    />\r\n                    <br />\r\n                    {win? <div id = 'feedback' className = 'status'>{victory}</div> : <div className = 'status'>{status}</div>}\r\n                    <br />\r\n\r\n                    <div className = 'win'>\r\n                        {(win || stepNumber === 9) && <button  className = 'reset' onClick = {()=>handleReset()}>Play Again?</button>}\r\n                        <br></br><br></br>{(win || stepNumber === 9) && <Link to={'/'}>\r\n                        <button className = 'button2'>Exit</button>\r\n                        </Link>}\r\n                    </div>\r\n                    <br></br>\r\n                    \r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Game;","C:\\Users\\manpe\\Documents\\year 2\\sem 1\\react projects\\websockets-experimental\\src\\client\\components\\Board.js",["38"],"import React, { Component } from 'react'\r\nimport Square from '../components/Square'\r\n\r\nconst Board = (props) => {\r\n    const renderSquare = (i) => {\r\n        return (\r\n            <Square \r\n                onClick = {() => props.onClick(i)}\r\n                value = {props.squares[i]}\r\n            />\r\n        )\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <div className = 'row'>\r\n                {renderSquare(0)}\r\n                {renderSquare(1)}\r\n                {renderSquare(2)}\r\n            <